# Git Operations

alias clo='git clone'
alias sta='git status'
alias fet='git fetch'
alias pul='git pull'
alias pik='git cherry-pick -s'
alias pus='git push'
alias log='git log'
alias logl='git log --oneline'
alias com='git commit -s'
alias rev='git revert -s'
alias res='git reset'
alias squ='git rebase -i'
alias mer='git merge'
alias add='git add'
alias dif='git diff'
alias bra='git branch'
alias che='git checkout'
alias abop='git cherry-pick --abort'
alias abor='git revert --abort'
alias ame='git commit -s --amend --author'
alias sho='git remote show'
alias fetu='git fetch upstream'
alias pat='git format-patch -1 HEAD'
